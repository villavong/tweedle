<%= form_for ([@board, @post]) do |f| %>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>
  <div class="field">
    <%= f.label "Upload Photo" %><br>
    <%= f.file_field :photo %>
  </div>
  <%= f.submit "Create Post", class: "button_pink button_highlight button_block" %>
<% end %>
